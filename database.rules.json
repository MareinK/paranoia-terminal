{
  "rules": {
    ".read": "false",
    ".write": "false",
    "waiting": {
      "$uid": {
        ".read": "$uid == auth.uid",
        ".write": "$uid == auth.uid"
      }
    },
    "signout": {
      "$uid": {
        ".read": "$uid == auth.uid"
      }
    },
    "messages": {
      "$uid": {
        ".read": "$uid == auth.uid"
      }
    }
  }
}